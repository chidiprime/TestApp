@{
    ViewData["Title"] = "Single Sign On";

}
@*<div id="LoadingImage" style="display: none">
    <img src="~/images/sosIcon.png" />
</div>*@

<form>

    <div class="form-group">
        <label>Grant_type1</label>
        <input class="form-control" id="grant_type" style="width:400px" />
    </div>
    <div class="form-group">
        <label>Username</label>
        <input class="form-control" id="username" style="width:400px" />
    </div>
    <div class="form-group">
        <label>Password</label>
        <input class="form-control" id="password" style="width:400px" />
    </div>
    <div class="form-group">
        <label>Scope</label>
        <input class="form-control" id="scope" style="width:400px" />
    </div>
    <div class="form-group">
        <label>Client ID</label>
        <input class="form-control" id="clientid" style="width:400px" />
    </div>
    <div class="form-group">
        <label>Client Secret</label>
        <input class="form-control" id="clientsecret" style="width:400px" />
    </div>
    <button type="button" id="save" class="btn btn-primary">Submit</button>
    <hr />
    <textarea id="result" style="height:200px; width:500px"></textarea>
</form>

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/Test.js"></script>
